{
  "name": "dagsnap-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev-snap": "concurrently \"next dev\" \"cd snap && yarn start\"",
    "install-all": "./scripts/install-dependencies.sh",
    "watch": "concurrently \"yarn --cwd snap start\" \"yarn --cwd proxy-server dev\" \"next dev\"",
    "build": "NEXT_PUBLIC_DISABLE_ESLINT=true next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@metamask/providers": "^17.2.0",
