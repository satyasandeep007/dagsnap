    "ethers": "^5.7.2",
    "next": "^14.2.9",
    "node-fetch": "^3.3.0",
    "qrcode.react": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^10.0.5",
    "typescript": "^5.6.2",
    "wagmi": "^0.9.6"
  },
  "devDependencies": {
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@eslint/js": "^9.10.0",
    "@metamask/eslint-config": "^13.0.0",
    "@metamask/eslint-config-jest": "^13.0.0",
    "@metamask/eslint-config-nodejs": "^13.0.0",
